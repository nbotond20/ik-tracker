{
  "name": "szakdolgozat-t3",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "lint-staged": "lint-staged --verbose --config lint-staged.config.cjs",
    "lint:fix": "eslint \"**/*.{ts,tsx,json,cjs,mjs,js,jsx}\" --quiet --fix",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:ci:report": "jest --ci --reporters=default --reporters=jest-junit",
    "preinstall": "npx only-allow pnpm",
    "cypress:open": "cypress open --e2e",
    "cypress": "cypress run",
    "db-seed": "cross-env NODE_ENV='development' prisma db seed",
    "typecheck": "tsc --noEmit --incremental"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
